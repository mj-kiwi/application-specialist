{
    "name": "mjliang/application-specialist",
    "description": "Application Specialist Test",
    "authors": [
        {
            "name": "MJ Liang",
            "email": "me@mj.kiwi"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "sempro/phpunit-pretty-print": "^1.0",

        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "MjLiang\\ApplicationSpecialist\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MjLiang\\ApplicationSpecialist\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "phpcbf": "./vendor/bin/phpcbf"
    }
}
